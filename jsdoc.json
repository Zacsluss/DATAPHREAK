{
  "source": {
    "include": ["src/js/utils"],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "destination": "./docs/api",
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/minami"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "markdown": {
    "parser": "gfm"
  }
}
